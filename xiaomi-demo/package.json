{
  "name": "taxflow-pro",
  "version": "1.0.0",
  "description": "TaxFlow Pro - 智能税务发票处理系统",
  "private": true,
  "scripts": {
    "dev": "cross-env NODE_ENV=development next dev -H 0.0.0.0 -p 3000",
    "build": "cross-env NODE_ENV=production next build",
    "start": "pm2 start config/ecosystem.config.js",
    "start:sit": "pm2 start config/ecosystem.config.js --env sit",
    "start:test": "pm2 start config/ecosystem.config.js --env test",
    "status": "pm2 status taxflow-pro",
    "stop": "pm2 stop taxflow-pro",
    "delete": "pm2 delete taxflow-pro",
    "restart": "pm2 restart taxflow-pro",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "type-check": "tsc --noEmit",
    "build:analyze": "cross-env ANALYZE=true npm run build",
    "export": "next export"
  },
  "dependencies": {
    "@ant-design/icons": "^6.0.2",
    "@ant-design/v5-patch-for-react-19": "^1.0.3",
    "@react-google-maps/api": "^2.19.3",
    "@types/lodash": "^4.17.20",
    "antd": "^5.27.4",
    "cross-env": "^7.0.3",
    "lodash": "^4.17.21",
    "next": "^15.4.6",
    "next-intl": "^4.3.5",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-dropzone": "^14.3.8",
    "reactflow": "^11.11.4",
    "recharts": "^3.0.2",
    "redis": "^5.8.2",
    "remixicon": "^4.6.0",
    "typescript": "^5",
    "winston": "^3.17.0"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@types/redis": "^4.0.10",
    "@typescript-eslint/eslint-plugin": "^8.39.1",
    "@typescript-eslint/parser": "^8.39.1",
    "autoprefixer": "^10.4.21",
    "eslint": "^9.33.0",
    "eslint-config-next": "^15.4.6",
    "postcss": "^8.5.5",
    "tailwindcss": "^3.4.17"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/taxflow-pro/frontend.git"
  },
  "keywords": [
    "tax",
    "invoice",
    "automation",
    "nextjs",
    "typescript",
    "tailwindcss"
  ],
  "author": "TaxFlow Pro Team",
  "license": "MIT"
}
