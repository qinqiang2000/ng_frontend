    },
    "dependencies": {
        "egg": "^3.21.0",
        "egg-cors": "^2.2.3",
        "egg-logger": "^2.4.1",
        "egg-multipart": "^2.13.1",
        "egg-mysql": "^3.2.0",
        "egg-redis": "^2.4.0",
        "egg-scripts": "^2.11.0",
        "egg-security": "^2.9.0",
