{
  "name": "{{projectName}}",
  "description": "{{projectDescription}}",
  "author": "金蝶票据云(深圳)有限公司",
  "version": "0.0.1",
  "private": true,
  "main": "dist/client/main.js",
  "dependencies": {
    "electron-updater": "^4.6.5"
  },
  "scripts": {
    "dev": "pwy-webpack5 --dev --progress --config=./buildConfig.js",
    "build": "pwy-webpack5 --build --config=./buildConfig.js",
    "build-fix": "pwy-webpack5 --build --fix-eslint --config=./buildConfig.js",
    "dev:client": "tsc && cross-env NODE_ENV=development DEV_SERVER_PORT=9002 electron ./dist/client/main.js",
    "start": "tsc && electron ./dist/client/main.js",
    "make": "tsc && node ./src/build/clear.js && electron-builder --config=./src/build/electron-builder-config.js"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@piaozone.com/electron-reloader": "^1.2.4",
    "@testing-library/jest-dom": "^5.16.2",
    "@testing-library/react": "^12.1.3",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.4.0",
    "@types/node": "^17.0.19",
    "@types/react": "^17.0.39",
    "@types/react-dom": "^17.0.11",
    "antd": "^4.18.8",
    "electron": "^17.1.0",
    "electron-builder": "^22.14.13",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "web-vitals": "^2.1.4"
  }
}
