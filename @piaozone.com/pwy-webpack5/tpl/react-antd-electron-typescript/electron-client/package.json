{
  "name": "electron-client",
  "productName": "electron-client",
  "version": "1.0.0",
  "description": "My Electron application description",
  "main": "dist/client/main.js",
  "scripts": {
    "dev-web": "npm run dev-web --prefix=../electron-web",
    "build-web": "npm run build-web --prefix=../electron-web",
    "build-fix-web": "npm run build-fix-wev --prefix=../electron-web",
    "dev": "tsc && cross-env NODE_ENV=development DEV_SERVER_PORT=9002 electron-forge start",
    "compile": "tsc",
    "start": "tsc && cross-env NODE_ENV=production electron-forge start",
    "package": "cross-env NODE_ENV=production electron-forge package",
    "premake": "npm run compile",
    "make": "cross-env NODE_ENV=production electron-forge make",
    "publish": "cross-env NODE_ENV=production electron-forge publish",
    "lint": "eslint --ext .ts ."
  },
  "keywords": [],
  "author": "kingdee",
  "license": "MIT",
  "devDependencies": {
    "@electron-forge/cli": "^6.0.0-beta.63",
    "@electron-forge/maker-deb": "^6.0.0-beta.63",
    "@electron-forge/maker-rpm": "^6.0.0-beta.63",
    "@electron-forge/maker-squirrel": "^6.0.0-beta.63",
    "@electron-forge/maker-zip": "^6.0.0-beta.63",
    "@piaozone.com/electron-reloader": "^1.2.3",
    "@typescript-eslint/eslint-plugin": "^5.12.1",
    "@typescript-eslint/parser": "^5.12.1",
    "electron": "17.1.0",
    "eslint": "^8.10.0",
    "eslint-plugin-import": "^2.25.4",
    "typescript": "~4.5.4"
  },
  "dependencies": {
    "electron-squirrel-startup": "^1.0.0"
  }
}
